{
  "name": "career-portal",
  "version": "3.2.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "node dist/server",
    "serve": "ng serve --configuration=static",
    "build": "npm run build:ssr",
    "build:static": "ng build --prod --configuration=static --aot --build-optimizer",
    "test": "ng test",
    "lint": "ng lint",
    "lint:fix": "npm run ng -- lint --typeCheck --format stylish --fix",
    "e2e": "ng e2e",
    "compile:server": "webpack --config webpack.server.config.js --progress --colors",
    "build:ssr": "npm run build:client-and-server-bundles && npm run compile:server",
    "build:client-and-server-bundles": "ng build --prod --configuration=dynamic && ng run career-portal:server:dynamic"
  },
  "jest": {
    "preset": "jest-preset-angular",
    "setupFilesAfterEnv": [
      "<rootDir>/setupJest.ts"
    ],
    "globals": {
      "ts-jest": {
        "tsConfig": "<rootDir>/tsconfig.json",
        "diagnostics": true,
        "stringifyContentPathRegex": "\\.html$",
        "astTransformers": [
          "jest-preset-angular/build/InlineFilesTransformer",
          "jest-preset-angular/build/StripStylesTransformer"
        ]
      }
    },
    "collectCoverage": true,
    "coverageReporters": [
      "json",
      "lcov",
      "text",
      "text-summary",
      "html"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "setupJest.ts"
    ],
    "testURL": "http://localhost"
  },
  "lint-staged": {
    "*.{ts,json,scss,css,html}": [
      "prettier --write",
      "git add"
    ]
  },
  "private": true,
  "engines": {
    "node": "10.3.0"
  },
  "dependencies": {
    "@angular/animations": "^8.2.14",
    "@angular/cdk": "^7.1.1",
    "@angular/common": "^8.2.14",
    "@angular/compiler": "^8.2.14",
    "@angular/core": "^8.2.14",
    "@angular/forms": "^8.2.14",
    "@angular/http": "^7.1.2",
    "@angular/platform-browser": "^8.2.14",
    "@angular/platform-browser-dynamic": "^8.2.14",
    "@angular/platform-server": "^8.2.14",
    "@angular/router": "^8.2.14",
    "@bullhorn/bullhorn-icons": "2.3.0",
    "@bullhorn/bullhorn-types": "^1.9.0",
    "@bullhorn/dragula": "1.0.1",
    "@nguniversal/express-engine": "^7.1.1",
    "@nguniversal/module-map-ngfactory-loader": "0.0.0",
    "ace-builds": "1.3.1",
    "angular2-text-mask": "^9.0.0",
    "body-parser": "^1.18.2",
    "brace": "^0.11.1",
    "bullhorn-icons": "^1.17.0",
    "chomsky": "^3.3.0",
    "classlist.js": "^1.1.20150312",
    "core-js": "^2.4.1",
    "date-fns": "^1.30.1",
    "express": "^4.16.2",
    "he": "^1.2.0",
    "jsontoxml": "^1.0.1",
    "novo-elements": "4.3.9",
    "post-robot": "^8.0.29",
    "roboto-fontface": "^0.9.0",
    "rxjs": "^6.6.3",
    "rxjs-compat": "^6.0.0-rc.0",
    "string-replace-webpack-plugin": "^0.1.3",
    "text-mask-addons": "^3.8.0",
    "typescript": "3.5.3",
    "xmlhttprequest": "^1.8.0",
    "zone.js": "^0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.803.18",
    "@angular-devkit/build-ng-packagr": "~0.803.17",
    "@angular/cli": "^8.3.17",
    "@angular/compiler-cli": "~8.2.13",
    "@angular/language-service": "~8.2.13",
    "@types/jasmine": "~2.8.3",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "^10.12.18",
    "codelyzer": "^5.0.1",
    "jasmine-core": "~3.5.0",
    "jest": "~26.4.2",
    "jest-preset-angular": "~8.3.1",
    "lint-staged": "^8.1.0",
    "protractor": "~5.1.2",
    "ts-loader": "^5.2.0",
    "ts-node": "~4.1.0",
    "tslint": "^5.11.0",
    "webpack": "^4.41.2",
    "webpack-cli": "^4.1.0"
  }
}
